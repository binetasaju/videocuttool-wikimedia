	"scripts": {
		"prestart": "node ./server/config.js",
		"start": "vite",
		"start:docker": "docker-compose -f ./docker-compose.dev.yml up --build",
		"build": "vite build",
		"test": "vitest",
		"postinstall": "npm --prefix ./server install",
